      "types": "./dist/components/*/index.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "description": "FE base form library",
  "scripts": {
    "build": "tsdown",
    "lint": "biome check --write",
    "prepare": "husky",
    "story": "ladle serve",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mavetic/mavetic-fe-core": "0.0.13",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/material-nextjs": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/x-date-pickers": "^8.7.0",
    "next": "^16.0.0",
    "@tanstack/react-form": "^1.14.1",
    "date-fns": "^4.1.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@ladle/react": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.14",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react": "^5.0.0",
    "happy-dom": "^20.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "tsdown": "^0.16.4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
