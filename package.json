{
  "name": "@mavetic/mavetic-fe-form",
  "version": "0.0.14",
  "packageManager": "pnpm@10.22.0",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
